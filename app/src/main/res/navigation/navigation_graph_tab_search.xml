<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph_tab_3"
    app:startDestination="@id/search">

    <navigation
        android:id="@+id/navigation"
        app:startDestination="@id/login">
        <fragment
            android:id="@+id/login"
            android:name="com.timenoteco.timenote.view.loginFlow.Login"
            android:label="fragment_login"
            tools:layout="@layout/fragment_login">
            <action
                android:id="@+id/action_login_to_signup"
                app:destination="@id/signup" />
            <argument
                android:name="x"
                app:argType="boolean" />
        </fragment>
        <fragment
            android:id="@+id/preferenceSubCategory"
            android:name="com.timenoteco.timenote.view.loginFlow.PreferenceSubCategory"
            android:label="fragment_preference_sub_category"
            tools:layout="@layout/fragment_preference_sub_category">
            <action
                android:id="@+id/action_preferenceSubCategory_to_preferenceSuggestion"
                app:destination="@id/preferenceSuggestion" />
            <argument
                android:name="isInLogin"
                app:argType="boolean" />
        </fragment>
        <fragment
            android:id="@+id/preferenceSuggestion"
            android:name="com.timenoteco.timenote.view.loginFlow.PreferenceSuggestion"
            android:label="fragment_preference_suggestion"
            tools:layout="@layout/fragment_preference_suggestion">
            <argument
                android:name="isInLogin"
                app:argType="boolean" />
        </fragment>
        <fragment
            android:id="@+id/preferenceCategory"
            android:name="com.timenoteco.timenote.view.loginFlow.PreferenceCategory"
            android:label="fragment_preference_category"
            tools:layout="@layout/fragment_preference_category">
            <action
                android:id="@+id/action_preferenceCategory_to_preferenceSubCategory"
                app:destination="@id/preferenceSubCategory" />
            <argument
                android:name="isInLogin"
                app:argType="boolean" />
        </fragment>
        <fragment
            android:id="@+id/signup"
            android:name="com.timenoteco.timenote.view.loginFlow.Signup"
            android:label="Signup">
            <action
                android:id="@+id/action_signup_to_preferenceCategory"
                app:destination="@id/preferenceCategory" />
        </fragment>
    </navigation>
    <fragment
        android:id="@+id/search"
        android:name="com.timenoteco.timenote.view.searchFlow.Search"
        android:label="Search">
        <action
            android:id="@+id/action_search_to_searchTop"
            app:destination="@id/searchTop" />
        <action
            android:id="@+id/action_search_to_searchExplore"
            app:destination="@id/searchExplore" />
        <action
            android:id="@+id/action_search_to_navigation"
            app:destination="@id/navigation" />
        <action
            android:id="@+id/action_search_to_searchExploreClicked"
            app:destination="@id/searchExploreClicked" />
        <action
            android:id="@+id/action_search_to_profileSearch"
            app:destination="@id/profileSearch" />
        <action
            android:id="@+id/action_search_to_timenoteAddressSearch"
            app:destination="@id/timenoteAddressSearch" />
        <action
            android:id="@+id/action_search_to_detailedTimenoteSearch"
            app:destination="@id/detailedTimenoteSearch" />
        <action
            android:id="@+id/action_search_to_createTimenoteSearch"
            app:destination="@id/createTimenoteSearch" />
    </fragment>
    <fragment
        android:id="@+id/searchExplore"
        android:name="com.timenoteco.timenote.view.searchFlow.SearchExplore"
        android:label="SearchExplore" />
    <fragment
        android:id="@+id/searchExploreClicked"
        android:name="com.timenoteco.timenote.view.searchFlow.SearchExploreClicked"
        android:label="SearchExploreClicked" >
        <action
            android:id="@+id/action_searchExploreClicked_to_profileSearch"
            app:destination="@id/profileSearch" />
        <argument
            android:name="category"
            app:argType="com.timenoteco.timenote.model.Category"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/searchTop"
        android:name="com.timenoteco.timenote.view.searchFlow.SearchTop"
        android:label="SearchTop" />
    <fragment
        android:id="@+id/detailedTimenoteSearch"
        android:name="com.timenoteco.timenote.view.searchFlow.DetailedTimenoteSearch"
        android:label="detailedTimenoteSearch"
        tools:layout="@layout/fragment_detailed_fragment">
        <action
            android:id="@+id/action_detailedTimenoteSearch_to_profileSearch"
            app:destination="@id/profileSearch" />
        <action
            android:id="@+id/action_detailedTimenoteSearch_to_createTimenoteSearch"
            app:destination="@id/createTimenoteSearch" />
        <argument
            android:name="timenoteInfoDTO"
            app:argType="com.timenoteco.timenote.model.TimenoteInfoDTO"
            app:nullable="true" />
        <deepLink
            android:id="@+id/deepLink"
            app:uri="https://timenote.app.link/owfJ8Houjab" />
    </fragment>
    <fragment
        android:id="@+id/profileSearch"
        android:name="com.timenoteco.timenote.view.searchFlow.ProfileSearch"
        android:label="ProfileSearch" >
        <action
            android:id="@+id/action_profileSearch_to_followPageSearch"
            app:destination="@id/followPageSearch" />
        <action
            android:id="@+id/action_profileSearch_to_detailedTimenoteSearch"
            app:destination="@id/detailedTimenoteSearch" />
        <action
            android:id="@+id/action_profileSearch_to_profileCalendarSearch"
            app:destination="@id/profileCalendarSearch" />
        <action
            android:id="@+id/action_profileSearch_to_createTimenoteSearch"
            app:destination="@id/createTimenoteSearch" />
        <argument
            android:name="userInfoDTO"
            app:argType="com.timenoteco.timenote.model.UserInfoDTO"
            app:nullable="true" />
        <action
            android:id="@+id/action_profileSearch_to_profilModifySearch"
            app:destination="@id/profilModifySearch" />
    </fragment>
    <fragment
        android:id="@+id/followPageSearch"
        android:name="com.timenoteco.timenote.view.searchFlow.FollowPageSearch"
        android:label="FollowPageSearch" />
    <fragment
        android:id="@+id/previewTimenoteCreatedSearch"
        android:name="com.timenoteco.timenote.view.searchFlow.PreviewTimenoteCreatedSearch"
        android:label="PreviewTimenoteCreatedSearch">
        <argument
            android:name="from"
            app:argType="integer" />
        <argument
            android:name="timenoteInfoDTO"
            app:argType="com.timenoteco.timenote.model.TimenoteInfoDTO"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/profileCalendarSearch"
        android:name="com.timenoteco.timenote.view.searchFlow.ProfileCalendarSearch"
        android:label="ProfileCalendarSearch" />
    <fragment
        android:id="@+id/createTimenoteSearch"
        android:name="com.timenoteco.timenote.view.createTimenoteSearchFlow.CreateTimenoteSearch"
        android:label="CreateTimenoteSearch" >
        <action
            android:id="@+id/action_createTimenoteSearch_to_previewTimenoteCreatedSearch"
            app:destination="@id/previewTimenoteCreatedSearch" />
        <argument
            android:name="modify"
            app:argType="integer" />
        <argument
            android:name="id"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="timenoteBody"
            app:argType="com.timenoteco.timenote.model.CreationTimenoteDTO"
            app:nullable="true" />
        <argument
            android:name="from"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/timenoteAddressSearch"
        android:name="com.timenoteco.timenote.view.searchFlow.TimenoteAddressSearch"
        android:label="TimenoteAddressSearch" >
        <argument
            android:name="timenoteInfoDTO"
            app:argType="com.timenoteco.timenote.model.TimenoteInfoDTO"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/profilModifySearch"
        android:name="com.timenoteco.timenote.view.searchFlow.ProfilModifySearch"
        android:label="ProfilModifySearch" >
        <argument
            android:name="isNotMine"
            app:argType="boolean" />
        <argument
            android:name="userInfoDTO"
            app:argType="com.timenoteco.timenote.model.UserInfoDTO"
            app:nullable="true" />
    </fragment>
</navigation>