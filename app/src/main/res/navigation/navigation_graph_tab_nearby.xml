<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph_tab_2"
    app:startDestination="@id/nearBy">
    <fragment
        android:id="@+id/nearBy"
        android:name="com.timenoteco.timenote.view.nearByFlow.NearBy"
        android:label="NearBy" >
        <action
            android:id="@+id/action_nearBy_to_nearbyFilters"
            app:destination="@id/nearbyFilters" />
        <action
            android:id="@+id/action_nearBy_to_comments"
            app:destination="@id/comments" />
        <action
            android:id="@+id/action_nearBy_to_detailedTimenote"
            app:destination="@id/detailedTimenote" />
        <action
            android:id="@+id/action_nearBy_to_profile"
            app:destination="@id/profile" />
        <action
            android:id="@+id/action_nearBy_to_timenoteAddress"
            app:destination="@id/timenoteAddress" />
        <action
            android:id="@+id/action_nearBy_to_createTimenote"
            app:destination="@id/createTimenote" />
    </fragment>
    <fragment
        android:id="@+id/nearbyFilters"
        android:name="com.timenoteco.timenote.view.nearByFlow.NearbyFilters"
        android:label="fragment_nearby_filters"
        tools:layout="@layout/fragment_nearby_filters" >
        <action
            android:id="@+id/action_nearbyFilters_to_nearBy"
            app:popUpTo="@id/nearBy" />
    </fragment>
    <fragment
        android:id="@+id/comments"
        android:name="com.timenoteco.timenote.view.homeFlow.Comments"
        android:label="Comments" />
    <fragment
        android:id="@+id/detailedTimenote"
        android:name="com.timenoteco.timenote.view.homeFlow.DetailedTimenote"
        android:label="fragment_detailed_fragment"
        tools:layout="@layout/fragment_detailed_fragment" >
        <action
            android:id="@+id/action_detailedTimenote_to_comments"
            app:destination="@id/comments" />
    </fragment>
    <fragment
        android:id="@+id/profile"
        android:name="com.timenoteco.timenote.view.profileFlow.Profile"
        android:label="Profile" >
        <argument
            android:name="whereFrom"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/timenoteAddress"
        android:name="com.timenoteco.timenote.view.homeFlow.TimenoteAddress"
        android:label="fragment_timenote_address"
        tools:layout="@layout/fragment_timenote_address" />
    <fragment
        android:id="@+id/createTimenote"
        android:name="com.timenoteco.timenote.view.createTimenoteFlow.CreateTimenote"
        android:label="fragment_create_timenote"
        tools:layout="@layout/fragment_create_timenote" >
        <action
            android:id="@+id/action_createTimenote_to_previewTimenoteCreated2"
            app:destination="@id/previewTimenoteCreated" />
        <argument
            android:name="modify"
            app:argType="boolean" />
        <argument
            android:name="id"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="timenoteBody"
            app:argType="com.timenoteco.timenote.model.TimenoteBody"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:id="@+id/previewTimenoteCreated"
        android:name="com.timenoteco.timenote.view.createTimenoteFlow.PreviewTimenoteCreated"
        android:label="fragment_preview_timenote_created"
        tools:layout="@layout/fragment_preview_timenote_created" />
</navigation>